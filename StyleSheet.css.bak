* {
	margin: 0;
	padding: 0;
}

.nav {
	display: flex;
	justify-content: space-between;
	align-items: center;
	padding: 20px;
	background-color: black;
	position: fixed;
	width: 100%;
}

.images {
	width: 150px;
	height: 150px;
}

.nav ul li {
	display: inline-block;
	list-style-type: none;
	margin-left: 20px;
	padding-right: 50px;
}

.nav ul li a{
	text-decoration: none;
	color: white;
	font-weight:bold;
}

.nav ul li a:hover{
	color:orange;
	
